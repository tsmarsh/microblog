{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "BlockList",
  "type": "object",
  "properties": {
    "user_id": {
      "type": "string",
      "format": "uuid",
      "description": "Identifier for the user doing the blocking"
    },
    "blocked_id": {
      "type": "string",
      "format": "uuid",
      "description": "Identifier for the user being blocked"
    }
  },
  "required": ["user_id", "blocked_id"]
}
